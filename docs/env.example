# Supabase
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Provider danych rynkowych (po MVP — opcjonalne, wykorzystywane przez /api/prices)
# Jeśli METALS_API_KEY jest ustawiony, backend spróbuje pobrać bieżącą cenę XAUUSD z Metals.dev API
# i użyje jej jako punktu startowego do generowania mockowego szeregu OHLC.
METALS_API_KEY=              # klucz API do Metals.dev (opcjonalny)
METALS_API_URL=https://api.metals.dev/v1/latest
METALS_CACHE_TTL_MS=43200000 # cache w ms (domyślnie 12h; ogranicza liczbę zapytań do Metals.dev)

# Aplikacja
NODE_ENV=development
APP_URL=http://localhost:3000
JWT_COOKIE_NAME=supabase-auth

