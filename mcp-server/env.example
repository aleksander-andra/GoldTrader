# API Keys dla zewnętrznych usług finansowych
# Skopiuj do .env i dodaj swoje klucze API

# Alpha Vantage (bezpłatne API dla danych finansowych)
# ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# News API (dla wiadomości finansowych)
# NEWS_API_KEY=your_news_api_key_here

# Yahoo Finance API (alternatywa)
# YAHOO_FINANCE_API_KEY=your_yahoo_key_here

# Open Exchange Rates (kursy walut)
# OPEN_EXCHANGE_RATES_APP_ID=your_app_id_here

# Konfiguracja
NODE_ENV=development
LOG_LEVEL=info
